/* table TWO neirong left*/
.timeaxis{
    padding-left: 0;
    padding-right: 0;
}
.timeaxis .timeline {
    position: relative;
    text-align: center;
    background: #fff;
    border-radius: 3px;
    padding-bottom: 2px;
}
.kongbai{
    height: 80px;
    background: #fff;
}
.timeline__item {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
    width: 50%;
    position: relative;
    margin-bottom: 60px;
}
@media screen and (min-width: 640px){
    .timeline__item--left {
        text-align: right;
    }
}
@media screen and (min-width: 640px){
    .timeline__item--right {
        left: 50%;
        text-align: left;
    }
}
@media screen and (min-width: 640px){
    .timeline__item:before {
        content: "";
        position: absolute;
        background-color:#e0f5ff;
        width: 20px;
        height: 20px;
        border-radius: 50%;
        top: -5px;
    }
}

.timeline__item:before {
    -webkit-transition: all 600ms cubic-bezier(0.175, 0.885, 0.335, 1);
    -moz-transition: all 600ms cubic-bezier(0.175, 0.885, 0.335, 1);
    -ms-transition: all 600ms cubic-bezier(0.175, 0.885, 0.335, 1);
    -o-transition: all 600ms cubic-bezier(0.175, 0.885, 0.335, 1);
    transition: all 600ms cubic-bezier(0.175, 0.885, 0.335, 1);
}
@media screen and (min-width: 640px){
    .timeline__item--left:before {
        right: -10px;
        z-index: 20;
    }
}

@media screen and (min-width: 640px){
    .timeline__item--right:before {
        left: -10px;
        z-index: 20;
    }
}
.timeline__item .link {
    z-index: 2;
    color: #020621;
}
.timeline__item .link .turn-off{
    text-decoration: underline;
    color: #101010;
    font-weight: bold;
}
@media screen and (min-width: 640px){
    .timeline__item--left .link {
        padding-right: 55px;
        position: relative;
        right: -30px;
    }
}

.grid-post {
    display: block;
}
.timeline__item:hover::before{
    -webkit-transform:scale(1.2);
    -moz-transform:scale(1.2);
    -o-transform:scale(1.2);
}
.grid-post__date {

    font-style: normal;
}
@media screen and (min-width: 640px){
    .timeline__item--right .link {
        padding-left: 55px;
        position: relative;
        left: -30px;
    }
}
.timeline:after {
    content: "";
    position: absolute;
    height: 100%;
    width: 1px;
    background-color: #F1EEEA;

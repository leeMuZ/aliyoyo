	margin-right:3px;
}
.thing .fancy_button .fanciers_count{
	font-size:14px;
	text-align: right;
}

.thumbnail--article{
	background-color:#fff;
}
.thumbnail--article .thumbnail-cover{
	display:block;
	overflow:hidden;
	width: 25%;
	float: left;
}
.thumbnail--article .thumbnail-cover img{
	width:100%;
}
.thumbnail--article .thumbnail-body{
	background-color:#fff;
	height:110px;
	padding:0 20px;
	position:relative;
	float: left;
	width:75%;
}
.thumbnail--article .thumbnail-body::before{
	border-color:transparent transparent #fff transparent;
	border-style:solid;
	border-width:10px;
	content:'';
	position:absolute;
	right:40px;
	top:-20px
}

.thumbnail--article .thumbnail-body .metas .name a{
	display:block;
	color:#777;
	max-width:10em;
}
.thumbnail--article .thumbnail-body .metas .name a:hover{
	text-decoration:none;
}
.thumbnail--article .thumbnail-title{
	font-size:16px;
	margin:0 0 10px 0;
	max-height:50px;
}
.thumbnail--article .thumbnail-title>a{
	color:#777;
	line-height: 23px;
	word-break:break-all; 
	word-wrap:break-word;
	display: inline-block;	
}
.thumbnail--article .thumbnail-label{
	left:0;
	position:absolute;
	top:-44px;
}
.thumbnail--article .thumbnail-label>label{
	background-color:#36383a;
	color:#fff;
	font-size:16px;
	height:44px;
	line-height:44px;
	margin:0;
	padding:0 1.5em;
}

.list-styled,.list-styled,.list_styled,.list_styled{
	margin-bottom:10px;
	padding-left:40px;
}
.list-styled,.list_styled{
	list-style-type:decimal;
}
.list-styled,.list_styled{
	list-style-type:disc;
}


.thing .cover .cover-inner{
	display:block;
	height:0;
	padding-bottom:100%;
}
.thing .content h4{
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	margin:0;
	font-size:14px;
	min-height:20.0px;
}
.thing .content h4 a{
	color:#333;
}

.thing .info:before,.thing .info:after{content:" ";display:table}
.thing .info:after{clear:both}
.thing .info .stats{
	float:left;
	max-width:57%;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	margin-top:6px;
}
.thing .info .stats>*{
	margin-right:.5em;
	color:#101010;
}
.thing .info .stats .fanciers_count{
	display:none;
}
@media (min-width: 992px){
	.thing .info .stats .fanciers_count{
		display:inline
	}

}
.newfooter-down_ul li:last-child{
	margin-right: 0;
}
.newfooter-down_ul_li_div input,.newfooter-down_ul_li_div .btn{
	float: left;
	height: 40px;
	margin: 0;
    border-radius: 5px;
    margin-right: 20px;
    font-size: 14px;
}
.newfooter-down_ul_li_div{
	margin-bottom: 10px;
    display: table;
    width: 100%;
    margin-top: 15px
}
.newfooter-down_ul_li_div input{
	width: 280px;
}
.newfooter-down_ul_li_div .btn{
	background: #26cada;
	color: #fff;
	font-weight: bold;
	border:none;
}
.newfooter-icon{
	border-top: 1px solid #bbb;
	padding-top: 30px;
}
.newfooter-icon_img{
	float: left;
	width: 120px;
	height: 45px;
	overflow: hidden;
}
.newfooter-icon_icon{
	float: right;
}
.newfooter-icon_icon  a{
	float: left;
	margin-right: 30px;
	margin-top: 22px;
}
.newfooter-icon_icon  a i{
	font-size: 25px;
	color: #fff;
}
h6{
	text-align: center;
	margin-top: 40px;

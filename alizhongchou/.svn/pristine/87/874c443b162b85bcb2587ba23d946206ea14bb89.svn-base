.chk_1, .chk_2, .chk_3, .chk_4 {
    display: none;
}
.chk_1:checked + label {
    background-color: #fff;
    border: 1px solid #313131;
    color: #313131;
}
.chk_1 + label {
    background-color: #FFF;
    border: 1px solid #313131;
    border-radius: 0px;
    display: inline-block;
    position: relative;
    margin-right: 30px;
    text-align: center;
    width: 16px;
    height: 16px;
    margin-right: 10px;
}
.chk_1+ label:hover{
	border-color: #86bde9;
}
.chk_1:checked + label:hover{
	border-color: #86bde9!important;
	
}
.chk_1:checked + label:after {
    content: '√';
    position: absolute;
    top: -3px;
    left: -5px;
    color: #313131;
    width: 100%;
    text-align: center;
    font-size: 1.6em;
    vertical-align: text-top;
}
.chk_1:checked + label:hover:after{
	color: #86bde9;
}

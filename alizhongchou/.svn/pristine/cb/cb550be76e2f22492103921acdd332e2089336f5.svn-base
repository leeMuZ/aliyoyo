@import url("animate-tool.css");
body {
    background: #f5f5f5;
    font-family: "Microsoft YaHei","Segoe UI", "Lucida Grande", Helvetica, Arial,sans-serif;
    font-size: 14px;
    color:#353535;
    margin: 0;
    padding: 0;
}
*{margin:0;padding:0;list-style-type:none;}
a,img{border:0;text-decoration:none;}
.l{
    float: left;
}
.r{
    float:right;
}
img{
    display: block;
    width:100%;
    height:100%;
}
/* --------------------------banner 图片bufen--------------------------- */
.home-banner{
    height: 650px;
   /*  display: none; */
   margin-bottom: 60px;
}
.home-banner .swiper-container {
    width: 100%;
    height: 100%;
    margin-left: auto;
    margin-right: auto;
}
.home-banner .swiper-slide {
    text-align: center;
    font-size: 18px;
    background: #fff;
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.home-banner .swiper-slide img{
    display: block;
    width: 100%;
    height: 100%;

}
.home-banner .swiper-button-next,.home-banner .swiper-button-prev{
    background-image: none;
}
.qiehuan{
    font-size: 30px;
    color: #fff;
}
.home-banner .swiper-pagination-bullet{
    background: #fff;
    opacity: 1;
}
.home-banner .swiper-pagination-bullet-active {
    background: #a8a8a8;
}
/* --------------------------banner 图片bufen end--------------------------- */

/* --------------------------fanzhun tupian--------------------------- */
@media (max-width: 768px){
    .container {
        width: 100%;
        padding-left: 0px!important;
        padding-right: 0px!important;
    }
}

@media (min-width: 992px){
    .container {
        width: 970px;
    }
}
@media (min-width: 1200px){
    .container {
        width: 1170px;
    }
}
/* ============================guanggaowi=================== */
.guanggaowei{
    padding:0 ;
    margin-bottom: 30px;
}
.regularly-item-list li.first{
    width: 18%!important;
    height: 350px!important;
    
}
.regularly-item-list li{
    width: 26.33333%;
    height: 170px; 
    float: left; 
    display: inline; 
    position: relative;
    margin-right: 1%;
    margin-bottom: 10px;
}
.regularly-item-list li .flip-box img{
    display: block;
    width: 100%;
    height: 100%;
}

.regularly-item-list li.first .flip-box{
    width: 100%;
    height: 350px; 
    padding:150px 15px 0 15px;
}
.regularly-item-list li.first .flip-box p{
    font-weight: bold;
}
.regularly-item-list li.first .flip-box p span{
    padding-bottom: 5px;
}
.regularly-item-list li.first .flip-box p:last-child span:first-child{
    border-bottom: 1px solid;
} 
.regularly-item-list li .flip-box{
    width:100%; 
    height:170px;
    position: absolute;
    left: 0;
    top: 0; 
    overflow: hidden;

}
.yuanyou{
    background: #4cbcbc;

}
.turnoff{
     background: #fff;
     
}
.yuanyou p{
    color: #fff;
}
.turnoff p{
    color: #101010;
}
.yuanyou p:first-child{
    font-size: 20px;

}
.turnoff p:first-child{
    font-size: 20px;

}
.little-margin{
    margin-bottom: 5px;
}
.tupianfanzhuan{
    padding:20px 15px;
    background: #fff;
    
}
.tupianfanzhuan a:hover{
    color: #101010;
}
.tupianfanzhuan a p{
    font-weight: bold;
}
.tupianfanzhuan .progress{
    height: 10px;
    margin-bottom: 20px;
    border-radius: 20px;
    background-color: #e8e8e8;
    overflow: visible;
}
.tupianfanzhuan .progress .progress-bar{
    background: #26c6da; 
    position: relative;
    animation: animate-positive 2s;
    border-radius: 20px;
    box-shadow: none;
}
/* InDemand */
.InDemand,.categories,.Marketplace,.Mostfunded{
    padding:0;
    margin-bottom: 30px;
}
.bottom{
     margin-left: -15px;

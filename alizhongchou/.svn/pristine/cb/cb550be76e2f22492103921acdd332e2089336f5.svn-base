}
.product .right{
    padding:0 10px 10px 10px;
}
.product-grid .product .name a{
    line-height: 22px;
}
.product-grid .product .name{
    height: auto;
    line-height: 22px;
    margin-bottom: 10px;
}

.product-grid .product{
    background: #fff;
    text-align: left;
   
}
.product-grid .product .name{
    margin-top: 10px;
    text-align: left;
}
.nduanluo{
    color: #757575;
    line-height: 18px;
    letter-spacing: 0;
    font-size: 13px;
    margin-bottom: 20px;
    max-height: 55px;
    overflow: hidden;
}
/* jidutiao */
.product-grid .product .progress{
    height: 10px;
    margin-bottom: 30px;
    border-radius: 20px;
    background-color: #e8e8e8;
    overflow: visible;
}
.product-grid .product .progress .progress-bar{
    background: #26c6da; 
    position: relative;
    animation: animate-positive 2s;
    border-radius: 20px;
    box-shadow: none;
}
.progress .progress-value{
    display: block;
    font-size: 13px;
    color: #101010;
    position: absolute;
    bottom: -25px;
    right: -15px;
}

@-webkit-keyframes animate-positive{
    0% { width: 0%; }
}
@keyframes animate-positive{
    0% { width: 0%; }
}
/* jindutiao end */
/* categories */
.categories-div{
    height: 245px;
    position: relative;
    margin-bottom: 30px;
}
.categories-div img{
    display: block;
    width: 100%;
 
    height: 100%;
}

.img-zhezhao{
    position: absolute;
    top: 0;
    left:0;
    width: 100%;
    height: 100%;
    padding-left: 15px;
    padding-right: 15px;
}
.zhezhaoimg{
    width: 100%;
    height: 100%;
    opacity:0.4;
    background: #000;
    z-index: 10;
    padding:5%;
}
.xianshidiv{
    width: 70%;
    height: 70%;
   /*  background: #fff; */
    position: absolute;
    top: 15%;
    left:15%;
    text-align: center;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    -ms-transition: all 0.2s ease-in-out;
    -o-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
    color: #fff;
    font-size: 16px;
}




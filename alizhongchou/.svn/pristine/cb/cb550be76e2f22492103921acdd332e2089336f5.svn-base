}

.product-grid{
    margin-top: 0;
   
}
.product-grid .product{
    padding-top: 0;
}
.product .left .heartlike{
    height: 30px;
    width: 30px;
    position: absolute;
    bottom: 20px;
    left: 15px;
    text-align: center;
    line-height: 35px;
    background: #e84e40;
    border-radius: 100%;
}
.product .left .heartlike i{
    color: #fff;
    font-size: 18px;
}
.product .left .deleted{
    height: 30px;
    width: 30px;
    position: absolute;
    top: 20px;
    right: 15px;
    display: none;
    text-align: center;
    line-height: 33px;
    background: #fff;
    border-radius: 100%;
    cursor: pointer;
}
.product .left .deleted i{
    font-size: 20px;
}
.acontantactive {
    background: #dedede!important;

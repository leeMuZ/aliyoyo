/*  */
.pagination-results{
	position: relative;
	padding: 0;
	margin: 0;
	overflow: auto;
	margin-top: 15px;
}
.pagination-results .text-left {
    padding: 0px 0px 0px 0px;
    width: 100%;
    text-align: center;
}
.pagination-results ul {
    padding: 0px;
    margin: 0px;
    list-style: none;
    padding: 10px 0px 10px 0px;
    margin-bottom: 20px;
}
.pagination-results ul li.active {
    background: #666666!important;
    color: #ffffff;
    
}
.pagination-results ul li {
	border: none;
    width: 40px;
    background: #fff;
    transition: all 0.2s;
    -webkit-transition: all 0.2s;
    -moz-transition: all 0.2s;
    text-align: center;
    
    font-size: 15px;
    line-height: 40px;
    height: 40px;
    font-weight: bold;
}
.pagination-results ul li a ,div.pagination-results ul li span{
    border: 0;
    width: 40px;
    height: 40px;
    line-height: 40px;
    padding: 0;
    border-radius: 0;
    color: #101010!important;
}
.pagination>li:first-child>a, .pagination>li:first-child>span{
	border-radius: 0;
	background: #666666!important;
	color: #fff!important;
}
.pagination>li:last-child>span {
   border-radius: 0;
}

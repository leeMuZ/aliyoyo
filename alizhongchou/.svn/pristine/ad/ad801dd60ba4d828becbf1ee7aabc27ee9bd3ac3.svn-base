ul{
	list-style: none;
}
.background{
	background: #fff;
}
.header{
	padding: 20px 15px 20px 15px;
}
.header h4{
	text-align: center;
    font-size: 36px;
    text-transform: none;
    color: #101010;
}
.product-grid .product{
	padding-top: 0;
	-webkit-box-shadow: 0 5px 10px 0 #eee;
    box-shadow: 0 5px 10px 0 #eee;
}
.select-top{
	margin-bottom: 20px;
}
.select_box{
	position:relative;
	cursor:pointer;
	border: 1px solid #26cada;
	height: 50px;
	border-radius: 3px;
}
.selet_open{
	display: inline-block;
    position: absolute;
    right: 0;
    top: 0;
    width: 30px;
    font-size: 24px;
    color: #bbb;
    line-height: 50px;
}
.selet_open i{
	color: #26cada;
	font-size: 25px;
    font-weight: bold;
}
.select_txt{
	display:inline-block;
	padding-left:10px;
	cursor:text;overflow:hidden;
	text-align: left!important;
	line-height: 50px;
    color: #26cada;
    font-size: 20px;
}
.option{
	background: #fff;
	width:100.3%;
	border:solid 1px #EDE7D6;
	position:absolute;
	top:40px;
	left:-1px;
	z-index:2;
	overflow:hidden;
	display:none;
	
}

.option ul{
	padding: 10px 0px;
    display: table;
    width: 100%;
}

.option ul li{
	max-height: 270px;
	overflow: auto;
	
}
.option ul li a:first-child{
	padding-left: 20px!important;
}
.option ul li a{
	display: block;
	padding: 10px 0px;
	font-size: 14px;
	font-weight: 500;
	padding-left: 50px;
}
.option ul li a:hover{
	color: #fff;
	background: #26cada;
}
.urnimgtwo{
	width: 33.33%;
	height: 170px;
}

.view-xiaohongmao {
    display: table;
    padding-top: 40px;
    width: 100%;
}

.xiaohongmao {
    background: url(../images/viewmore.png)no-repeat;
    width: 180px;
    margin: 0 auto;
    height: 86px;
    display: table;
    padding-top: 40px;
    text-align: center;
    margin-bottom: 60px;
}

.xiaohongmao .chas {
    color: #fff;
    letter-spacing: 1px;
    font-size: 16px;
    font-weight: bold;
}

.beauful-all{
	height: 600px;
}
.beautful{
	background:url(../images/button.png) no-repeat;
	height:590px;
	background-position: center;
	background-size: 100% 100%;
	top:13px;
	left:0;
}
@media screen and (min-width: 1200px){
.block_list>li, .block_list>.thing, .block_list>.thing_list, .block_list>.discussion {
    width: 33.33333%;
}
}
@media (min-width: 768px){
	.block_list{background-color:transparent;font-size:0;margin-left:auto;margin-right:auto;padding: 0;}
}
@media (min-width: 992px){
	.block_list{
	padding: 0;
 }
}
.block_list>li{
	margin-bottom:20px
}
@media (min-width: 768px){
	.block_list>li,.block_list>.activity:not(.activity--text),.block_list>.thing,.block_list>.thing_list,.block_list>.discussion{
		display:inline-block;font-size:14px;padding-left:15px;padding-right:15px;vertical-align:top
}
}
@media (min-width: 768px){
	.block_list>.activity:not(.activity--text){
	width:50%
}
}
@media (min-width: 1200px){
	.block_list>li,.block_list>.thing,.block_list>.thing_list,.block_list>.discussion{
	width:33.33333%
}
}
.block_list>.loading-things{
	padding-top:1em}.block_list.js-waterfall{padding-bottom:28px
	}
.block_list.js-waterfall>.loading-things,.block_list.js-waterfall>.nomore{
	bottom:0;font-size:14px;height:28px;padding-top:0;position:absolute
	}
.block_list.js-waterfall>.nomore{
	line-height:28px;padding:0;width:100%
    }
    
@media only screen and (min-width:768px) and (max-width:1200px) {
	.block_list>li{
	  width:50%!important;
}
}


@media screen and (min-width:320px) and (max-width:414px){
	.thanksgiving_banner img {
	    height: 200px;
	}

}

@media screen and (min-width:415px) and (max-width:768px){
	.thanksgiving_banner{
	    height: 600px;
	}
	.thanksgiving_banner img{
		height: 600px;
	}
	

}

@media screen and (max-width: 414px){
		.thanksgiving_banner{
			height: 240px;
			background-size: 100% 100%;
		}
}

@media screen and (min-width:320px) and (max-width:768px){
	.beauful-all{
			height: 240px;
		}
		.beautful{
			height:240px;
		}
}

@media(max-width:992px){
.thanksgiving_ul.container{
	padding:0 !important;
}
}

}
div.pagination-results ul li {
	border: none;
    width: 40px;
    background: #fff;
    transition: all 0.2s;
    -webkit-transition: all 0.2s;
    -moz-transition: all 0.2s;
    text-align: center;
    
    font-size: 15px;
    line-height: 40px;
    height: 40px;
    font-weight: bold;
}

div.pagination-results ul li a ,div.pagination-results ul li span{
    border: 0;
    width: 40px;
    height: 40px;
    line-height: 40px;
    padding: 0;
    border-radius: 0;
    color: #101010!important;
}
div.pagination-results ul li a:hover, div.pagination-results ul li span:hover{
	background:#a1a1a1!important;
	color: #fff!important;
}
.checkbox{display:none}
 .switch{
 width: 62px;
 height: 32px;
 background: #E5E5E5;
 z-index: 0;
 margin: 0;
 padding: 0;
 appearance: none;
 border: none;
 cursor: pointer;
 position: relative;
 border-radius:16px; //IE 11
 -moz-border-radius:16px; //Mozilla
 -webkit-border-radius:16px; //Chrome and Safari
 }
 .switch:before{
 content: ' ';
 position: absolute;
 left: 1px;
 top: 1px;
 width: 60px;
 height: 30px;
 background: #FFFFFF;
 z-index: 1;
 border-radius:16px; //IE 11
 -moz-border-radius:16px; //Mozilla
 -webkit-border-radius:16px; //Chrome and Safari
 }
 .switch:after {
 content: ' ';
 height: 29px;
 width: 29px;
 border-radius: 28px;
 background: #FFFFFF;
 position: absolute;
 z-index: 2;
 top: 1px;
 left: 1px;
 -webkit-transition-duration: 300ms;
 transition-duration: 300ms;
 -webkit-box-shadow: 0 2px 5px #999999;
 box-shadow: 0 2px 5px #999999;
 }
 .switchOn, .switchOn:before{
 background: #4cd964 !important;
 }
 .switchOn:after{
 left: 33px !important;
 }


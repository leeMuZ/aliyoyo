#newshopping_top_all .swiper-container {
    width: 100%;
    height: 460px;
    margin-left: auto;
    margin-right: auto;
    position: relative;
}
#newshopping_top_all .swiper-slide {
    text-align: center;
    font-size: 18px;
    height: 460px;
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}
#newshopping_top_all .swiper-slide img{
    bottom: -1000%;
    left: -1000%;
    margin: auto;
    max-width: 200%;
    min-height: 100%;
    position: absolute;
    right: -1000%;
    top: -1000%;
}

 #newshopping_top_all .swiper-button-next,  #newshopping_top_all .swiper-button-prev{
    width: 40px;
    height: 64px;
    background-image: none;
    font-size: 64px;
    color: #fff;
    text-align: center;
    line-height: 64px;
 }
.newshopping_top_all-swiper-button{
    background: #212121;
}
#newshopping_top_all .swiper-container-horizontal>.swiper-pagination-bullets, #newshopping_top_all .swiper-pagination-custom, #newshopping_top_all .swiper-pagination-fraction {
    position: absolute;
    bottom: 10px;
    left:90%;
    width: 100px;
}
 #newshopping_top_all  .swiper-button-prev{
    left: 250px;
}
 #newshopping_top_all{
    position: relative;
    padding:0;
 }


/* Delete your project modal */

@media screen and (min-width: 868px){
    .individual-modal .modal-dialog{
        width: 650px;
        margin-top: 150px;
    }
}
.individual-modal .modal-header{
    padding: 25px 15px 25px 30px;
    background: #f0f0f0;
    border-radius: 5px;
}
.individual-modal h4{
    padding:0;
    font-weight: bold;
    text-transform: none;
}
.close{
    opacity: 1;
}
.close span i{
    font-size: 20px;
}
.individual-modal .modal-body{
    padding: 15px 30px 10px 30px;
}
.individual-modal .modal-body p{
    font-size: 16px;
}
.individual-modal .modal-footer{
    border:none;
    text-align: left;
    padding-left: 30px;
    margin-top: 0;
}
.individual-modal .modal-footer .btn{
    margin: 0 0;
    border:none;
    text-transform: none; 
}
.individual-modal .modal-footer .btn:first-child{
    background:#fd003b;
    width: 120px;
    color: #fff;
    margin-right: 15px;
}
.individual-modal .modal-footer .btn:last-child:hover{
    background: none;
    color: #101010;
}
